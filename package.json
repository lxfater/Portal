{
  "name": "portal",
  "description": "Your best AI portal",
  "version": "1.0.10",
  "private": true,
  "author": {
    "email": "lxfater@gmail.com"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "rebuild": "electron-rebuild -f -w electron",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer && npm run build:chat",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "build:chat": "cd ./packages/chat && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js  --config.asar=true",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.mjs",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,vue",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,vue,json}\""
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.10",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/marked": "^4.0.8",
    "@types/node": "18.15.3",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/test-utils": "2.3.1",
    "cross-env": "7.0.3",
    "electron": "^23.1.4",
    "electron-builder": "23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "8.36.0",
    "eslint-plugin-vue": "9.9.0",
    "happy-dom": "8.9.0",
    "playwright": "1.31.2",
    "typescript": "4.9.5",
    "unplugin-auto-expose": "0.0.4",
    "vite": "4.1.4",
    "vitest": "0.29.2",
    "vue-tsc": "1.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@imengyu/vue3-context-menu": "^1.2.3",
    "active-win": "^8.1.0",
    "classic-level": "^1.2.0",
    "custom-electron-titlebar": "^4.2.0-beta.0",
    "electron-acrylic-window": "^0.5.11",
    "electron-selected-text": "^1.1.2",
    "electron-updater": "5.3.0",
    "element-plus": "^2.3.0",
    "eventsource-parser": "^0.1.0",
    "expiry-map": "^2.0.0",
    "graphology": "^0.25.1",
    "graphology-layout": "^0.6.1",
    "graphology-layout-forceatlas2": "^0.10.1",
    "graphology-layout-noverlap": "^0.4.2",
    "handlebars": "^4.7.7",
    "highlight.js": "^11.7.0",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.6",
    "marked": "^4.2.12",
    "node-fetch": "^2.6.9",
    "pinia": "^2.0.33",
    "sass": "^1.59.3",
    "sigma": "^3.0.0-alpha3",
    "ua-parser-js": "^1.0.34",
    "useragent": "^2.3.0",
    "uuid": "^9.0.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  }
}
